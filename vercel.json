{
    "version": 2,
    "builds": [
      {
        "src": "./**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/([^/]+)",
        "dest": "/$1.js"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }
  